      by {SIM:SetOfImport <- SIM:SetOfImport;}
:def sIMcsp = :csp {
  eq sIM = empIM .
  eq sIM = (im sIM') .
}
:apply (sIMcsp)
:apply (rd-)
:def isFIMctp = :ctf {
