  --    and allIMNotStablyWired(SIM,SCO0,SIM0)
  ceq (cid(id(CO)) in makeupIDBag(SIM,(CO SCO0),SIM0)) = false
    if canStart(id(CO),SCO0,SIM0) and
       allIMNotStablyWired(SIM,SCO0,SIM0) .

